		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.gogit.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.gogit.pr.model.vo.Pr" alias="Pr"/>
